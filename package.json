	"license": "ISC",
	"bugs": {
		"url": "https://github.com/interrupter/not-filter/issues"
	},
	"homepage": "https://github.com/interrupter/not-filter#readme",
	"dependencies": {
		"escape-string-regexp": "^1.0.5",
		"lower-case": "^1.1.4",
		"not-config": "0.0.3",
		"not-path": "*"
	},
	"devDependencies": {
		"chai": "*",
		"chai-as-promised": "*",
		"eslint": "*",
		"ink-docstrap": "^1.3.2",
		"istanbul": "*",
		"jsdoc": "^3.5.5",
		"mocha": "*"
	}
